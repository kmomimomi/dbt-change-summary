{
  "name": "dbt-change-summary",
  "version": "0.1.0",
  "description": "GitHub Action to analyze dbt project changes, generate PR comments, and Excel reports",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "all": "npm run build && npm run format && npm run lint && npm run test"
  },
  "keywords": [
    "github",
    "action",
    "dbt",
    "pr",
    "comment",
    "excel",
    "release"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.8.7",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nock": "^13.3.4",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}